<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleRobotDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="ModuleRobot1Dlg1 Module Dialog" Height="350" Width="300" Background="Gray" ShowInTaskbar="False">
    <Grid >
        <ScrollViewer Margin="0,0,0,55.667" HorizontalScrollBarVisibility="Auto">
            <TextBox x:Name="theTextBox" TextWrapping="NoWrap" Text="{Binding TheConfigString}" AcceptsReturn="True" />
        </ScrollViewer>
        <Button x:Name="OKButton" Content="OK " Margin="0,0,146.333,9.667" Height="28" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="115" Click="OKButton_Click" />
        <Button x:Name="CancelButton" Content="Cancel" Margin="0,0,26.333,9.667" Height="28" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="115" Click="CancelButton_Click"/>
    </Grid>

</local:ModuleBaseDlg>
